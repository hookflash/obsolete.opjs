<table>
	<tbody>
		<tr>
			<td>
				<p><a target="result-frame" href="/test">Run ALL tests</a></p>
				{{#each tests}}
					<h1>{{label}} Tests</h1>
					<ul>
						{{#each tests}}
							<li><a target="result-frame" href="{{url}}">{{label}}</a></li>
						{{/each}}
					</ul>
				{{/each}}

				<br/>
				<br/>
				<hr>
				<h1>Links</h1>
				<ul>
					<li><a target="_blank" href="http://chaijs.com/api/bdd/">chaijs expect/should</li>
					<li><a target="_blank" href="http://chaijs.com/api/assert/">chaijs assert</li>
				</ul>
			</td>
			<td>
				<iframe id="result-frame"></iframe>
			</td>
		</tr>
	</tbody>
</table>